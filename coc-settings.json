{
  "coc.preferences.formatOnSaveFiletypes": ["py", "yaml", "json", "python", "javascript", "typescript", "typescriptreact", "json", "javascriptreact", "typescript.tsx", "html"],
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "python.formatting.provider": "black",
